            <cost name="Warband Rating" typeId="wb-rating" value="0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="0258ef21-24e5-08f9-2e4e-c75fa0ff828d" name="1 - Chotec&apos;s Wrath" hidden="false" collective="false" import="true" type="upgrade">
