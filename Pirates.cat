        <selectionEntry type="upgrade" import="true" name="Speed Skills" hidden="false" id="8634-eb30-b5be-7b7b" collective="false">
          <constraints>
            <constraint type="max" value="1" field="selections" scope="parent" shared="true" id="42d4-374a-1572-b820" percentValue="false" includeChildSelections="false" includeChildForces="false"/>
