                  <entryGroups/>
                  <modifiers/>
                  <rules>
                    <rule id="4812e629-a542-e0ff-3edf-a807b1a0773d" name="Pit Fighter" hidden="false" page="0">
                      <description>The warrior has learned how to fight in enclosed spaces from his time in the dangerous fighting pits of the Empire. He is an expert at fighting in confined areas and adds +1 to his WS and +1 to his Attacks if he is fighting inside buildings or ruins.</description>
                      <modifiers/>
                    </rule>
                  </rules>
                  <profiles/>
                  <links/>
                </entry>
                <entry id="b6175506-41cf-ecee-327b-b6fc3ba68316" name="Resilient" points="0.0" categoryId="(No Category)" type="upgrade" minSelections="0" maxSelections="1" minInForce="0" maxInForce="-1" minInRoster="0" maxInRoster="-1" minPoints="0.0" maxPoints="-1.0" collective="false" hidden="false" page="0">
