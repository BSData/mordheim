                  <entryGroups/>
                  <modifiers/>
                  <rules>
                    <rule id="20d3ff44-676d-e416-1b1b-99a54bd2af24" name="Resilient" hidden="false" page="0">
                      <description>Deduct -1 Strength from all hits against him in close combat. This does not affect armour save modifiers</description>
                      <modifiers/>
                    </rule>
                  </rules>
                  <profiles/>
                  <links/>
                </entry>
