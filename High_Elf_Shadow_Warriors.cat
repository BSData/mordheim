                <entry id="ca57804c-dc5d-ef51-7654-c004a6cbbc82" name="Mighty Blow" points="0.0" categoryId="(No Category)" type="upgrade" minSelections="0" maxSelections="1" minInForce="0" maxInForce="-1" minInRoster="0" maxInRoster="-1" minPoints="0.0" maxPoints="-1.0" collective="false" hidden="false" page="0">
                  <entries/>
                  <entryGroups/>
                  <modifiers/>
                  <rules>
                    <rule id="4241b83c-2f1a-019d-78a4-792f268faea4" name="Mighty Blow" hidden="false" page="0">
                      <description>The warrior knows how to use his strength to maximum effect and has a +1 Strength bonus in close combat (excluding pistols). As his
Strength is used for close combat weapons, the bonus applies to all such weapons.</description>
                      <modifiers/>
                    </rule>
                  </rules>
                  <profiles/>
                  <links/>
                </entry>
                <entry id="15eb871e-6a2a-6ed0-4b05-dad3a29cd126" name="Pit Fighter" points="0.0" categoryId="(No Category)" type="upgrade" minSelections="0" maxSelections="1" minInForce="0" maxInForce="-1" minInRoster="0" maxInRoster="-1" minPoints="0.0" maxPoints="-1.0" collective="false" hidden="false" page="0">
