    <profile id="20ca-82d5-4ea4-4152" name="Dwarf max" hidden="false" typeId="e1beaa44-e54d-dd6b-d1f2-446b333c9bb9" typeName="Model">
      <characteristics>
        <characteristic name="M" typeId="2a0bcc4c-8266-418f-13d6-a6b44def5e92">3</characteristic>
        <characteristic name="WS" typeId="d5aca8ba-0204-b324-b976-c2b536e09924">7</characteristic>
        <characteristic name="BS" typeId="5b4d181b-23ae-5ed7-9262-c1d2f79246a8">6</characteristic>
        <characteristic name="S" typeId="7f1f0a4d-68dc-b0df-5703-c4d0d91a93b9">4</characteristic>
        <characteristic name="T" typeId="54f4796b-dedb-c296-8b1a-ff7f8043293a">5</characteristic>
