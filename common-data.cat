    <selectionEntryGroup id="e5ac-cd5b-9057-e096" name="Strength Skills" hidden="false" collective="false" import="true">
      <constraints>
        <constraint field="selections" scope="parent" value="0" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="eb1d-d235-c34f-c0c4" type="min"/>
      </constraints>
      <selectionEntries>
        <selectionEntry id="d36e-76e6-5849-ac6f" name="Mighty Blow" page="0" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="c4a4-d305-f8a4-d500" type="max"/>
          </constraints>
          <rules>
            <rule id="5968-9fb9-faf5-483a" name="Mighty Blow" page="0" hidden="false">
              <description>The warrior knows how to use his strength to maximum effect and has a +1 Strength bonus in close combat (excluding pistols). As his
Strength is used for close combat weapons, the bonus applies to all such weapons.</description>
            </rule>
          </rules>
          <costs>
            <cost name="pts" typeId="points" value="0"/>
            <cost name="Warband Rating" typeId="wb-rating" value="0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="4a90-2a9e-c488-c788" name="Pit Fighter" page="0" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="d7de-e806-f3db-c642" type="max"/>
          </constraints>
          <rules>
            <rule id="570a-1bf0-dda5-fd21" name="Pit Fighter" page="0" hidden="false">
              <description>The warrior has learned how to fight in enclosed spaces from his time in the dangerous fighting pits of the Empire. He is an expert at fighting in confined areas and adds +1 to his WS and +1 to his Attacks if he is fighting inside buildings or ruins.</description>
            </rule>
          </rules>
          <costs>
            <cost name="pts" typeId="points" value="0"/>
            <cost name="Warband Rating" typeId="wb-rating" value="0"/>
          </costs>
          <modifiers>
            <modifier type="set" value="true" field="hidden">
              <conditions>
                <condition type="instanceOf" value="1" field="selections" scope="force" childId="36fc-7d97-ad53-330b" shared="true"/>
              </conditions>
            </modifier>
          </modifiers>
        </selectionEntry>
        <selectionEntry id="0239-b9df-15c7-b504" name="Resilient" page="0" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="af10-e94b-d3bf-43aa" type="max"/>
          </constraints>
          <rules>
            <rule id="303e-4423-a930-89a1" name="Resilient" page="0" hidden="false">
              <description>Deduct -1 Strength from all hits against him in close combat. This does not affect armour save modifiers</description>
            </rule>
          </rules>
          <costs>
            <cost name="pts" typeId="points" value="0"/>
            <cost name="Warband Rating" typeId="wb-rating" value="0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="a4b7-a933-2ff7-7ba5" name="Fearsome" page="0" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="d11c-4dd8-43d5-5698" type="max"/>
          </constraints>
          <rules>
            <rule id="622b-5f14-8709-72bb" name="Fearsome" page="0" hidden="false">
              <description>The model that he causes fear in opposing models</description>
            </rule>
          </rules>
          <costs>
            <cost name="pts" typeId="points" value="0"/>
            <cost name="Warband Rating" typeId="wb-rating" value="0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="19cd-d2c3-587c-5cf4" name="Strongman" page="0" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="04e5-339d-cab2-fa1e" type="max"/>
          </constraints>
          <rules>
            <rule id="ed15-f754-5be6-c494" name="Strongman" page="0" hidden="false">
              <description>He may use a double-handed weapon without the usual penalty of always striking last. Work out order of battle as you would with other weapons</description>
            </rule>
          </rules>
          <costs>
            <cost name="pts" typeId="points" value="0"/>
            <cost name="Warband Rating" typeId="wb-rating" value="0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="e96f-e278-3662-6617" name="Unstoppable Charge" page="0" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="c190-cb44-2c43-f37e" type="max"/>
          </constraints>
          <rules>
            <rule id="7259-b051-c7b9-4345" name="Unstoppable Charge" page="0" hidden="false">
              <description>When he charges, the warrior is almost impossible to halt. He adds +1 to his Weapon Skill when charging.</description>
            </rule>
          </rules>
          <costs>
            <cost name="pts" typeId="points" value="0"/>
            <cost name="Warband Rating" typeId="wb-rating" value="0"/>
          </costs>
        </selectionEntry>
